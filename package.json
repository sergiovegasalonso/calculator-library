{
  "name": "@sergiovegasalonso/calculator-library",
  "version": "1.0.2",
  "description": "Calculator library.",
  "main": "dist-node/index.js",
  "browser": "dist-browser/index.js",
  "keywords": [
    "example",
    "calculator",
    "library"
  ],
  "author": "Sergio Vegas Alonso <sergio.vegas.alonso@gmail.com> (https://www.sergio-vegas-alonso.io)",
  "license": "MIT",
  "scripts": {
    "build:node": "npx tsc src/index.ts --outDir dist-node",
    "build:browser": "npx tsc src/index.ts --outDir dist-browser --module ES6",
    "build": "npm run build:node & npm run build:browser",
    "test": "vitest",
    "prettier": "$ npx prettier --write src/**",
    "lint:js": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lintfix": "prettier --write --list-different . & npm run lint:js --fix src/**",
    "precommit": "npm run lintfix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "tsc-files": "^1.1.4",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2"
  }
}
